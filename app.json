{
  "expo": {
    "name": "Horizon Walls",
    "slug": "horizonwalls",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/4.png",
    "scheme": "horizonwalls",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-logoicon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.sahil.horizonwalls"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
          
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4677981033286236~5504793911",
          "iosAppId": null
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "androidPackageName": "com.sahil.horizonwalls",
          "playStoreSigningType": "debug",
          "iosUrlScheme": "com.googleusercontent.apps.1234567890-fakeid"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0cbdfc73-8682-448c-ad50-eb6b5bc55971"
      }
    }
  }
}
