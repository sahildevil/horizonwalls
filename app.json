{
  "expo": {
    "name": "Horizon Walls",
    "slug": "horizonwalls",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/4.png",
    "scheme": "horizonwalls",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/5.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY",
        "MEDIA_LIBRARY_WRITE_ONLY"
      ],
      "package": "com.sahil.horizonwalls",
      "targetSdkVersion": 33
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/4.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4677981033286236~5504793911",
          "iosAppId": null
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "androidPackageName": "com.sahil.horizonwalls",
          "playStoreSigningType": "debug",
          "iosUrlScheme": "com.googleusercontent.apps.1234567890-fakeid"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Horizon Walls to access your photos.",
          "savePhotosPermission": "Allow Horizon Walls to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
